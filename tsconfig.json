{
  "compilerOptions": {
    "baseUrl": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "ESNext",
    "target": "ES6",
    "allowJs": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
    "strictNullChecks": true,
    "lib": [
      "DOM",
      "ES5",
      "ES6",
      "ES7"
    ],
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "outDir": "./",
    "esModuleInterop": true,
  },
  "include": [
    "**/*.ts",
    "src/view.tsx"
, "src/excalidraw-app/src/icons.tsx"  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}